<div class="page-header">
  <h3>User League #<%= @user_league.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @user_league.user_id %></dd>

      <dt>League</dt>
      <dd><%= @user_league.league_id %></dd>

      <dt>Commissioner</dt>
      <dd><%= @user_league.commissioner %></dd>

      <dt>Selected contestants</dt>
      <dd><%= @user_league.selected_contestants %></dd>

      <dt>Point total</dt>
      <dd><%= @user_league.point_total %></dd>

      <dt>Points last week</dt>
      <dd><%= @user_league.points_last_week %></dd>

      <dt>League locked</dt>
      <dd><%= @user_league.league_locked %></dd>

      <dt>Extra1</dt>
      <dd><%= @user_league.extra1 %></dd>

      <dt>Extra2</dt>
      <dd><%= @user_league.extra2 %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/user_leagues" class="btn btn-primary">
        Back
      </a>
      <a href="/user_leagues/<%= @user_league.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_user_league/<%= @user_league.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
