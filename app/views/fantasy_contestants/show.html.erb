<div class="page-header">
  <h3>Fantasy Contestant #<%= @fantasy_contestant.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Cast</dt>
      <dd><%= @fantasy_contestant.cast_id %></dd>

      <dt>User league</dt>
      <dd><%= @fantasy_contestant.user_league_id %></dd>

      <dt>Extra1</dt>
      <dd><%= @fantasy_contestant.extra1 %></dd>

      <dt>Extra2</dt>
      <dd><%= @fantasy_contestant.extra2 %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/fantasy_contestants" class="btn btn-primary">
        Back
      </a>
      <a href="/fantasy_contestants/<%= @fantasy_contestant.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_fantasy_contestant/<%= @fantasy_contestant.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
